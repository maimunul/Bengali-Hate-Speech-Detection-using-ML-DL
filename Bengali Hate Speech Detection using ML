    "import re\n",
    "import nltk\n",
    "from nltk.corpus import stopwords\n",
    "import string\n",
    "\n",
    "def transform_text(text):\n",
    "    text = nltk.word_tokenize(text)\n",
    "\n",
    "    y = []\n",
    "    for i in text:\n",
    "        i = re.sub(r'\\W+', '', i)\n",
    "        if i:\n",
    "            y.append(i)\n",
    "\n",
    "    text = y[:]\n",
    "    y.clear()\n",
    "    \n",
    "    for i in text:\n",
    "        if i not in stopwords.words('bengali') and i not in string.punctuation:\n",
    "            y.append(i)\n",
    "             \n",
    "    return \" \".join(y)"
